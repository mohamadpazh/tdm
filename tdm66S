<script>
    $('.mid-page-promo-btn-close').click(function () {
        $('.mid-page-promo-btn-wrapper').remove();
        document.cookie = "hide_mid_page_promo_btn=1;expires=0;domain=.investing.com;path=/";
        addMidPagePromoBtnEvent('ads_free_hook_closed', 'hook closed', 'close');
    });
    $('.mid-page-promo-btn-link').click(function () {
        addMidPagePromoBtnEvent('click_on_ads_free_hook', 'click on ads free hook', 'click');
    });

    function addMidPagePromoBtnEvent(eventNameCd, gaEventAction, actionCd, referralComponent = undefined) {
        var addMidPagePromoBtnEventObj = Object.assign({}, window.trackingObj);
        addMidPagePromoBtnEventObj.eventNameCd = eventNameCd;
        addMidPagePromoBtnEventObj.gaEventCategory = 'Ad-Free Subscription';
        addMidPagePromoBtnEventObj.gaEventAction = gaEventAction;
        addMidPagePromoBtnEventObj.gaEventLabel = 'side widget';
        addMidPagePromoBtnEventObj.actionCd = actionCd;
        addMidPagePromoBtnEventObj.trackedObject = 'side widget';
        addMidPagePromoBtnEventObj.uiTemplate = '1';
                                        if (eventNameCd === 'click_on_ads_free_hook') {
            addMidPagePromoBtnEventObj.referralComponent = 'side widget';
        }
        addMidPagePromoBtnEventObj.eventCdDescription1 = 'hook type';
        addMidPagePromoBtnEventObj.eventCdValue1 = 'side widget';

        window.addGAEvent(addMidPagePromoBtnEventObj);
    }

    $(function () {
        addMidPagePromoBtnEvent('ads_free_hook_impression', 'hook impression', 'impression');
    });
</script>

