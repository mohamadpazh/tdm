                displaySlot = (containerId) => {
                    window.dfpSlots = window.dfpSlots || [];
                    window.dfpSlotFlag = window.dfpSlotFlag || [];
                    if(typeof window.dfpFlag !== 'undefined') {
                        window.googletag.cmd.push(() => window.googletag.display(containerId))
